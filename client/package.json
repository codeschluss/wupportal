{
  "name": "@portal/client",
  "version": "0.0.3",
  "description": "Client for Wupportal: An informative portal for new citizens in Wuppertal",
  "homepage": "https://www.wupportal.org",
  "author": {
    "name": "Wupportal Team"
  },
  "bugs": {
    "email": "info@codeschluss.de",
    "url": "https://github.com/codeschluss/wupportal/issues"
  },
  "config": {
    "api": {
      "rootUrl": "/api",
      "authUrl": "/api/login",
      "refreshUrl": "/api/refresh"
    },
    "jwtClaims": {
      "activityProvider": "createdActivities",
      "organisationAdmin": "adminOrgas",
      "organisationUser": "approvedOrgas",
      "superUser": "superuser",
      "userId": "id"
    },
    "nominatim": {
      "city": "Wuppertal",
      "endpoint": "https://nominatim.openstreetmap.org/search",
      "params": "format=json&addressdetails=1"
    },
    "translations": {
      "defaultLocale": "de"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codeschluss/wupportal"
  },
  "scripts": {
    "build": "ng build",
    "start": "ng build --watch",
    "strip": "ng build --prod",
    "swgen": "ng-swagger-gen --config swagger.json",
    "xi18n": "ng xi18n"
  },
  "dependencies": {
    "@angular/animations": "7.2.1",
    "@angular/cdk": "7.2.1",
    "@angular/common": "7.2.1",
    "@angular/compiler": "7.2.1",
    "@angular/core": "7.2.1",
    "@angular/flex-layout": "7.0.0-beta.23",
    "@angular/forms": "7.2.1",
    "@angular/http": "7.2.1",
    "@angular/material": "7.2.1",
    "@angular/material-moment-adapter": "7.2.1",
    "@angular/platform-browser": "7.2.1",
    "@angular/platform-browser-dynamic": "7.2.1",
    "@angular/router": "7.2.1",
    "@angular/service-worker": "7.2.1",
    "@fortawesome/angular-fontawesome": "0.3.0",
    "@fortawesome/fontawesome-svg-core": "1.2.12",
    "@fortawesome/free-solid-svg-icons": "5.6.3",
    "@ngx-pwa/local-storage": "7.4.0",
    "@ngx-translate/i18n-polyfill": "1.0.0",
    "@portal/core": "file:libs/core",
    "@portal/forms": "file:libs/forms",
    "classlist.js": "1.1.20150312",
    "core-js": "2.6.2",
    "hammerjs": "2.0.8",
    "moment": "2.23.0",
    "ngx-hm-carousel": "1.6.0",
    "ngx-md": "7.0.0",
    "ngx-openlayers": "0.8.22",
    "resize-observer-polyfill": "1.5.1",
    "rrule": "2.6.0",
    "rxjs": "6.3.3",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.28"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.12.2",
    "@angular-devkit/build-ng-packagr": "0.12.2",
    "@angular/cli": "7.2.2",
    "@angular/compiler-cli": "7.2.1",
    "@angular/language-service": "7.2.1",
    "@types/node": "10.12.18",
    "codelyzer": "4.5.0",
    "ng-packagr": "4.6.0",
    "ng-swagger-gen": "1.4.2",
    "ts-node": "7.0.1",
    "tsickle": "0.34.0",
    "tslint": "5.12.1",
    "typescript": "3.2.2"
  }
}
