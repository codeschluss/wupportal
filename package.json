  "scripts": {
    "ng": "ng",
    "ns": "ns",
    "lint": "ng lint",
    "clean": "git clean -dfX",
    "start": "npm run start:app",
    "build:api": "ng-swagger-gen -c swagger.json",
    "build:app": "ng build --prod @wooportal/client",
    "build:ios": "ns platform add ios ||:&& ns build ios",
    "build:oid": "ns platform add android ||:&& ns build android",
    "build:srv": "ng run @wooportal/client:server --configuration=production",
    "build:sys": "for sys in sys/*; do ng build @wooportal/${sys#sys/}; done",
    "build:xlf": "ng xi18n && sed -i '/context/d' res/All/i18n/strings.xliff",
    "start:oid": "ns platform add android ||:&& ns debug android",
    "start:ios": "ns platform add ios ||:&& ns debug ios",
    "start:app": "ng serve --host 0.0.0.0 --port",
    "start:ssr": "node platforms/web/server"
  },
