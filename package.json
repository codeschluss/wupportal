{
  "name": "wupportal",
  "version": "0.1.0",
  "description": "An informative portal for new citizens in Wuppertal",
  "homepage": "https://www.wupportal.org",
  "author": {
    "name": "codeschluss",
    "email": "info@codeschluss.de",
    "url": "https://www.codeschluss.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codeschluss/wupportal"
  },
  "bugs": {
    "email": "info@codeschluss.de",
    "url": "https://github.com/codeschluss/wupportal/issues"
  },
  "config": {
    "api": {
      "rootUrl": "/api",
      "authUrl": "/api/login",
      "refreshUrl": "/api/refresh"
    },
    "jwtClaims": {
      "activityProvider": "createdActivities",
      "organisationAdmin": "adminOrgas",
      "organisationUser": "approvedOrgas",
      "superUser": "superuser",
      "userId": "id"
    },
    "defaults": {
      "appUrl": "https://www.wupportal.org",
      "city": "Wuppertal",
      "language": "de",
      "title": "Wupportal"
    }
  },
  "android": {
    "markingMode": "none",
    "v8Flags": "--expose_gc"
  },
  "nativescript": {
    "id": "de.codeschluss.wupportal",
    "tns-android": {
      "version": "6.2.0"
    },
    "tns-ios": {
      "version": "6.2.0"
    }
  },
  "main": "native.js",
  "scripts": {
    "ng": "ng",
    "tns": "tns",
    "lint": "ng lint",
    "clean": "git clean -dfX",
    "start": "npm run start:dev",
    "setup": "update-ns-webpack --configs && run-s setup:*",
    "build": "run-s setup build:app build:ssr build:ios build:oid",
    "build:api": "ng-swagger-gen --config swagger.json --input",
    "build:app": "ng build --base-href / --prod @wooportal/client",
    "build:ios": "tns build ios --bundle --env.aot --env.uglify",
    "build:oid": "tns build android --bundle --env.aot --env.uglify",
    "build:lib": "ng build @wooportal/core && ng build @wooportal/forms",
    "build:ssr": "ng run @wooportal/client:server && tsc -p express.json",
    "build:xlf": "ng xi18n && sed -i '/context/d' res/All/i18n/strings.xliff",
    "setup:env": "cp package.json src && cp tsconfig.json tsconfig.tns.json",
    "setup:ios": "tns platform add ios ||:&& tns prepare ios",
    "setup:oid": "tns platform add android ||:&& tns prepare android",
    "start:dev": "ng serve --host 0.0.0.0 --port",
    "start:ios": "tns debug ios --env.aot",
    "start:oid": "tns debug android --env.aot",
    "start:ssr": "node target/express"
  },
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/platform-server": "8.2.14",
    "@angular/router": "8.2.14",
    "@angular/service-worker": "8.2.14",
    "@fortawesome/angular-fontawesome": "0.5.0",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@nguniversal/common": "8.1.1",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngx-pwa/local-storage": "8.2.3",
    "@ngx-translate/i18n-polyfill": "1.0.0",
    "@wooportal/core": "file:libs/core",
    "@wooportal/forms": "file:libs/forms",
    "color-convert": "1.9.3",
    "express": "4.17.1",
    "marked": "0.7.0",
    "moment": "2.24.0",
    "ng-swagger-gen": "1.8.1",
    "ngx-openlayers": "1.0.0-next.13",
    "ol": "6.1.1",
    "rxjs": "6.5.3",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.20",
    "@angular-devkit/build-ng-packagr": "0.803.20",
    "@angular/cli": "8.3.20",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@nativescript/schematics": "0.7.3",
    "@types/node": "12.12.14",
    "classlist.js": "1.1.20150312",
    "codelyzer": "5.2.0",
    "core-js": "3.4.5",
    "hammerjs": "2.0.8",
    "nativescript": "6.2.2",
    "nativescript-angular": "8.20.3",
    "nativescript-dev-webpack": "1.3.0",
    "nativescript-geolocation": "5.1.0",
    "nativescript-ng-ripple": "2.0.1",
    "nativescript-social-share": "1.5.2",
    "nativescript-ui-calendar": "6.0.0",
    "nativescript-ui-sidedrawer": "8.0.0",
    "ng-packagr": "5.7.1",
    "normalize.css": "8.0.1",
    "npm-run-all": "4.1.5",
    "reflect-metadata": "0.1.13",
    "tns-core-modules": "6.2.3",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.4",
    "tsickle": "0.37.1",
    "tslib": "1.10.0",
    "tslint": "5.20.1",
    "typescript": "3.5.3",
    "web-animations-js": "2.3.2",
    "node-sass": "4.13.0"
  }
}
