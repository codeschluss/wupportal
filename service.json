{
  "index": "/",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/",
          "/manifest.json",
          "/*.js",
          "!/*-es5.js"
        ]
      }
    },
    {
      "name": "app-es5",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/*-es5.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/css/*.css",
          "/i18n/**",
          "/images/**",
          "/videos/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "/api/**",
        "!/api/login",
        "!/api/refresh"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "timeout":"500u",
        "maxAge": "6h"
      }
    }
  ]
}
