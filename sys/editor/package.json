{
  "name": "@wooportal/editor",
  "version": "0.1.3",
  "description": "@wooportal/client editor library",
  "homepage": "https://github.com/wooportal/client",
  "author": {
    "name": "codeschluss",
    "email": "info@codeschluss.de",
    "url": "https://www.codeschluss.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wooportal/client"
  },
  "bugs": {
    "email": "info@codeschluss.de",
    "url": "https://github.com/wooportal/client/issues"
  },
  "config": {
    "language": "en",
    "table": {
      "contentToolbar": [
        "tableColumn",
        "tableRow",
        "mergeTableCells"
      ]
    },
    "toolbar": {
      "items": [
        "heading",
        "alignment",
        "fontSize",
        "|",
        "bold",
        "italic",
        "underline",
        "code",
        "|",
        "strikethrough",
        "subscript",
        "superscript",
        "|",
        "bulletedList",
        "numberedList",
        "indent",
        "outdent",
        "|",
        "link",
        "blockQuote",
        "codeBlock",
        "insertTable",
        "horizontalLine",
        "|",
        "undo",
        "redo"
      ]
    }
  },
  "optionalDependencies": {
    "@angular-devkit/build-webpack": "0.803.24",
    "@angular/cli": "8.3.24",
    "@angular/core": "8.2.14",
    "@ckeditor/ckeditor5-alignment": "16.0.0",
    "@ckeditor/ckeditor5-angular": "1.1.2",
    "@ckeditor/ckeditor5-basic-styles": "16.0.0",
    "@ckeditor/ckeditor5-block-quote": "16.0.0",
    "@ckeditor/ckeditor5-code-block": "16.0.0",
    "@ckeditor/ckeditor5-core": "16.0.0",
    "@ckeditor/ckeditor5-dev-utils": "12.0.7",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "8.0.7",
    "@ckeditor/ckeditor5-editor-inline": "16.0.0",
    "@ckeditor/ckeditor5-essentials": "16.0.0",
    "@ckeditor/ckeditor5-font": "16.0.0",
    "@ckeditor/ckeditor5-heading": "16.0.0",
    "@ckeditor/ckeditor5-horizontal-line": "16.0.0",
    "@ckeditor/ckeditor5-indent": "16.0.0",
    "@ckeditor/ckeditor5-link": "16.0.0",
    "@ckeditor/ckeditor5-list": "16.0.0",
    "@ckeditor/ckeditor5-paragraph": "16.0.0",
    "@ckeditor/ckeditor5-paste-from-office": "16.0.0",
    "@ckeditor/ckeditor5-table": "16.0.0",
    "@ckeditor/ckeditor5-theme-lark": "16.0.0",
    "postcss-loader": "3.0.0",
    "raw-loader": "4.0.0",
    "style-loader": "1.1.3",
